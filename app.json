{
  "expo": {
    "name": "fitAI",
    "slug": "fitai",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "scheme": "fitaiapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitapp.ai",
      "buildNumber": "NaN",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access",
        "NSPhotoLibraryUsageDescription": "This app needs photo library access"
      },
      "appleTeamId": "JVCM6ZKG36"
    },
    "android": {
      "package": "com.fitai.health",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE",
        "SYSTEM_ALERT_WINDOW"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "fitaiapp",
              "host": "ssocallback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "softwareKeyboardLayoutMode": "resize",
      "packageVisibility": [
        "*"
      ],
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT_UP"
        }
      ],

 [
      "@react-native-voice/voice"
    ],
      "expo-web-browser",
      "expo-speech-recognition"
    ],
    "extra": {
      "eas": {
        "projectId": "09473377-c82b-4265-a7b5-69c4ad326759"
      }
    }
  }
}